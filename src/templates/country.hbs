<div class="one__country">
    {{#each this}}
    <ul class="country__descr">
        <li>{{name}}</li>
        <li>Capital: {{capital}}</li>
        <li>Population: {{population}} people</li>
        <li>Languages: <ul>
            {{#each languages}}
            <li>{{this.name}}</li>
            {{/each}}
        </ul></li>
        <li><img src={{flags.svg}} width="64" height="32"></li>
    </ul>
    {{/each}}
</div>